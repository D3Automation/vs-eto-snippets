<?xml version="1.0" encoding="utf-8" ?>
<CodeSnippets  xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>Insert Code Region</Title>
      <Shortcut>enregion</Shortcut>
      <Description>Code snippet for creating a code region</Description>
      <Author>D3 Automation</Author>
      <SnippetTypes>
				<SnippetType>Expansion</SnippetType>
			</SnippetTypes>
		</Header>
		<Snippet>
			<Declarations>
				<Literal>
					<ID>regionName</ID>
					<Default>My Region</Default>
				</Literal>
			</Declarations>
			<Code Language="IntentV3">
				<![CDATA['==============================================================
	'   $regionName$
	'==============================================================
	$end$]]>
			</Code>
		</Snippet>
	</CodeSnippet>
</CodeSnippets>